{
  "name": "kaggle",
  "n_gpu": 1,
  "arch": {
    "sinc_out_channels": 20,
    "sinc_kernel_size" : 1024,
    "ResBlocks" : [[20, 20], [20, 20], [20, 128], [128, 128], [128, 128], [128, 128]],
    "gru_input_size" : 128,
    "gru_hidden_size" : 1024,
    "gru_num_layers" : 3
  },
  "data": {
    "train": {
      "batch_size": 32,
      "num_workers": 8,
      "datasets": [
        {
          "type": "ASVDataset",
          "args": {
            "data_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_train/flac",
            "protocols_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.train.trn.txt"
          }
        }
      ]
    },
    "dev": {
      "batch_size": 32,
      "num_workers": 8,
      "datasets": [
        {
          "type": "ASVDataset",
          "args": {
            "data_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_dev/flac",
            "protocols_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.dev.trl.txt"
          }
        }
      ]
    },
    "eval": {
      "batch_size": 32,
      "num_workers": 8,
      "datasets": [
        {
          "type": "ASVDataset",
          "args": {
            "data_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_eval/flac",
            "protocols_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.eval.trl.txt"
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 0.0001,
      "weight_decay" : 0.0001
    }
  },
  "metrics": [
    {
      "type": "EER",
      "args": {
        "name": "EER"
      }
    }
  ],
  "lr_scheduler": {
    "type": "OneCycleLR",
    "args": {
      "steps_per_epoch": 100,
      "epochs": 100,
      "anneal_strategy": "cos",
      "max_lr": 1e-2,
      "pct_start": 0.2
    }
  },
  "trainer": {
    "epochs": 100,
    "save_dir": "saved/",
    "save_period": 5,
    "verbosity": 2,
    "monitor": "min dev_loss",
    "early_stop": 100,
    "visualize": "wandb",
    "wandb_project": "rawnet_project",
    "len_epoch": 100,
    "grad_norm_clip": 10
  }
}
